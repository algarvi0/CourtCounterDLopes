<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary theme color of the app (sets background color of app bar) -->
        <item name="colorPrimary">#FF9800</item>
        <!-- Background color of buttons in the app -->
        <item name="colorButtonNormal">#FF9800</item>

    </style>

    <style name="ThemeAwesomeButtonColor" parent="AppTheme">
        <item name="colorPrimary">@color/material_orange_600</item>
        <item name="colorButtonNormal">@color/material_orange_600</item>
    </style>
</resources>


